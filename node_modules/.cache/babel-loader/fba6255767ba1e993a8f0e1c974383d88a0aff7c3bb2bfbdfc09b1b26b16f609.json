{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport TodoList from '@/components/TodoList';\nimport AddTodo from '@/components/AddTodo';\nimport Loader from '@/components/Loader';\nimport Logout from \"@/components/Logout.vue\";\nexport default {\n  data() {\n    return {\n      todos: [],\n      loading: true,\n      filter: 'all'\n    };\n  },\n  components: {\n    TodoList,\n    AddTodo,\n    Loader,\n    Logout\n  },\n  mounted() {\n    // fetch('https://jsonplaceholder.typicode.com/todos?_limit=10')\n    //     .then(response => response.json())\n    //     .then(json => {\n    //       this.todos = json\n    //       this.loading = false\n    //     })\n  },\n  computed: {\n    filteredTodos() {\n      if (this.filter === 'all') {\n        return this.todos;\n      }\n      if (this.filter === 'completed') {\n        return this.todos.filter(t => t.completed);\n      }\n      if (this.filter === 'not-completed') {\n        return this.todos.filter(t => !t.completed);\n      }\n    }\n  },\n  methods: {\n    removeTodo(id) {\n      this.todos = this.todos.filter(t => t.id !== id);\n    },\n    addTodo(todo) {\n      this.todos.push(todo);\n    }\n  }\n};","map":{"version":3,"names":["TodoList","AddTodo","Loader","Logout","data","todos","loading","filter","components","mounted","computed","filteredTodos","t","completed","methods","removeTodo","id","addTodo","todo","push"],"sources":["src/views/Todos.vue"],"sourcesContent":["<template>\n <div class=\"main\">\n   <h2>Todo application</h2>\n   <router-link to=\"/\">Home Page</router-link>\n   <hr>\n   <AddTodo\n       v-bind:todos=\"todos\"\n       @add-todo=\"addTodo\"\n   />\n   <select v-model=\"filter\">\n     <option value=\"all\">All</option>\n     <option value=\"completed\">Completed</option>\n     <option value=\"not-completed\">Not completed</option>\n   </select>\n   <hr>\n\n   <Loader v-if=\"loading\" />\n\n   <TodoList\n       v-else-if=\"filteredTodos.length\"\n       v-bind:todos=\"filteredTodos\"\n       @remove-todo=\"removeTodo\"\n   />\n\n   <p v-else>No TODOS!</p>\n\n<Logout />\n\n </div>\n</template>\n\n<script>\nimport TodoList from '@/components/TodoList';\nimport AddTodo from '@/components/AddTodo';\nimport Loader from '@/components/Loader';\nimport Logout from \"@/components/Logout.vue\";\nexport default {\n  data() {\n    return {\n      todos: [],\n      loading: true,\n      filter: 'all'\n    }\n  },\n  components : {\n    TodoList,\n    AddTodo,\n    Loader,\n    Logout,\n  },\n  mounted() {\n    // fetch('https://jsonplaceholder.typicode.com/todos?_limit=10')\n    //     .then(response => response.json())\n    //     .then(json => {\n    //       this.todos = json\n    //       this.loading = false\n    //     })\n  },\n  computed : {\n    filteredTodos() {\n      if(this.filter === 'all') {\n        return this.todos\n      }\n      if (this.filter === 'completed') {\n        return this.todos.filter(t => t.completed)\n      }\n      if(this.filter === 'not-completed') {\n       return this.todos.filter(t => !t.completed)\n      }\n    }\n  },\n  methods : {\n    removeTodo(id) {\n      this.todos= this.todos.filter(t => t.id !== id)\n    },\n    addTodo(todo) {\n      this.todos.push(todo)\n    }\n  }\n}\n</script>\n\n"],"mappings":";AAgCA,OAAAA,QAAA;AACA,OAAAC,OAAA;AACA,OAAAC,MAAA;AACA,OAAAC,MAAA;AACA;EACAC,KAAA;IACA;MACAC,KAAA;MACAC,OAAA;MACAC,MAAA;IACA;EACA;EACAC,UAAA;IACAR,QAAA;IACAC,OAAA;IACAC,MAAA;IACAC;EACA;EACAM,QAAA;IACA;IACA;IACA;IACA;IACA;IACA;EAAA,CACA;EACAC,QAAA;IACAC,cAAA;MACA,SAAAJ,MAAA;QACA,YAAAF,KAAA;MACA;MACA,SAAAE,MAAA;QACA,YAAAF,KAAA,CAAAE,MAAA,CAAAK,CAAA,IAAAA,CAAA,CAAAC,SAAA;MACA;MACA,SAAAN,MAAA;QACA,YAAAF,KAAA,CAAAE,MAAA,CAAAK,CAAA,KAAAA,CAAA,CAAAC,SAAA;MACA;IACA;EACA;EACAC,OAAA;IACAC,WAAAC,EAAA;MACA,KAAAX,KAAA,QAAAA,KAAA,CAAAE,MAAA,CAAAK,CAAA,IAAAA,CAAA,CAAAI,EAAA,KAAAA,EAAA;IACA;IACAC,QAAAC,IAAA;MACA,KAAAb,KAAA,CAAAc,IAAA,CAAAD,IAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module