{"ast":null,"code":"export default {\n  name: 'App',\n  data() {\n    return {\n      player: {\n        x: 100,\n        y: 100,\n        size: 50,\n        color: 'blue',\n        speed: 5\n      }\n    };\n  },\n  mounted() {\n    this.initCanvas();\n    this.startGame();\n  },\n  methods: {\n    initCanvas() {\n      this.canvas = this.$refs.gameCanvas;\n      this.context = this.canvas.getContext('2d');\n    },\n    startGame() {\n      this.update();\n      window.addEventListener('keydown', this.handleKeyDown);\n    },\n    handleKeyDown(event) {\n      switch (event.key) {\n        case 'ArrowUp':\n          this.player.y -= this.player.speed;\n          break;\n        case 'ArrowDown':\n          this.player.y += this.player.speed;\n          break;\n        case 'ArrowLeft':\n          this.player.x -= this.player.speed;\n          break;\n        case 'ArrowRight':\n          this.player.x += this.player.speed;\n          break;\n      }\n    },\n    update() {\n      this.clearCanvas();\n      this.drawPlayer();\n      requestAnimationFrame(this.update);\n    },\n    clearCanvas() {\n      this.context.clearRect(0, 0, this.canvas.width, this.canvas.height);\n    },\n    drawPlayer() {\n      this.context.fillStyle = this.player.color;\n      this.context.fillRect(this.player.x, this.player.y, this.player.size, this.player.size);\n    }\n  },\n  beforeDestroy() {\n    window.removeEventListener('keydown', this.handleKeyDown);\n  }\n};","map":{"version":3,"names":["name","data","player","x","y","size","color","speed","mounted","initCanvas","startGame","methods","canvas","$refs","gameCanvas","context","getContext","update","window","addEventListener","handleKeyDown","event","key","clearCanvas","drawPlayer","requestAnimationFrame","clearRect","width","height","fillStyle","fillRect","beforeDestroy","removeEventListener"],"sources":["src/views/Game.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <canvas ref=\"gameCanvas\" width=\"800\" height=\"600\"></canvas>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'App',\n  data() {\n    return {\n      player: {\n        x: 100,\n        y: 100,\n        size: 50,\n        color: 'blue',\n        speed: 5\n      }\n    };\n  },\n  mounted() {\n    this.initCanvas();\n    this.startGame();\n  },\n  methods: {\n    initCanvas() {\n      this.canvas = this.$refs.gameCanvas;\n      this.context = this.canvas.getContext('2d');\n    },\n    startGame() {\n      this.update();\n      window.addEventListener('keydown', this.handleKeyDown);\n    },\n    handleKeyDown(event) {\n      switch (event.key) {\n        case 'ArrowUp':\n          this.player.y -= this.player.speed;\n          break;\n        case 'ArrowDown':\n          this.player.y += this.player.speed;\n          break;\n        case 'ArrowLeft':\n          this.player.x -= this.player.speed;\n          break;\n        case 'ArrowRight':\n          this.player.x += this.player.speed;\n          break;\n      }\n    },\n    update() {\n      this.clearCanvas();\n      this.drawPlayer();\n      requestAnimationFrame(this.update);\n    },\n    clearCanvas() {\n      this.context.clearRect(0, 0, this.canvas.width, this.canvas.height);\n    },\n    drawPlayer() {\n      this.context.fillStyle = this.player.color;\n      this.context.fillRect(this.player.x, this.player.y, this.player.size, this.player.size);\n    }\n  },\n  beforeDestroy() {\n    window.removeEventListener('keydown', this.handleKeyDown);\n  }\n};\n</script>\n\n<style>\n#app {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  height: 100vh;\n  background-color: #f0f0f0;\n}\ncanvas {\n  border: 1px solid black;\n}\n</style>\n"],"mappings":"AAOA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,MAAA;QACAC,CAAA;QACAC,CAAA;QACAC,IAAA;QACAC,KAAA;QACAC,KAAA;MACA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,UAAA;IACA,KAAAC,SAAA;EACA;EACAC,OAAA;IACAF,WAAA;MACA,KAAAG,MAAA,QAAAC,KAAA,CAAAC,UAAA;MACA,KAAAC,OAAA,QAAAH,MAAA,CAAAI,UAAA;IACA;IACAN,UAAA;MACA,