vuex';\n\nexport default {\n  data() {\n    return {\n      register: {\n        email: '',\n        password: '',\n      },\n      passwordType: 'password', // Default to 'password' type\n    };\n  },\n  methods: {\n    ...mapActions({\n      registerUser: 'auth/registerOrLogin',\n      setAuthInputValue: 'auth/setAuthInputValue',\n      setChangePage: 'auth/setChangePage',\n      setRegisterOrLoginValue: 'auth/setRegisterOrLoginValue',\n    }),\n    updateAuthInputValue() {\n      this.setAuthInputValue(this.register);\n    },\n    togglePasswordVisibility() {\n      this.passwordType = this.passwordType === 'password' ? 'text' : 'password';\n    },\n  },\n  mounted() {\n    this.setChangePage('/todos');\n    console.log(this.setChangePage())\n    this.setRegisterOrLoginValue('/register');\n  }\n}\n</script>\n"],"mappings":"AA6BA,SAAAA,UAAA;AAEA;EACAC,KAAA;IACA;MACAC,QAAA;QACAC,KAAA;QACAC,QAAA;MACA;MACAC,YAAA;IACA;EACA;EACAC,OAAA;IACA,GAAAN,UAAA;MACAO,YAAA;MACAC,iBAAA;MACAC,aAAA;MACAC,uBAAA;IACA;IACAC,qBAAA;MACA,KAAAH,iBAAA,MAAAN,QAAA;IACA;IACAU,yBAAA;MACA,KAAAP,YAAA,QAAAA,YAAA;IACA;EACA;EACAQ,QAAA;IACA,KAAAJ,aAAA;IACAK,OAAA,CAAAC,GAAA,MAAAN,aAAA;IACA,KAAAC,uBAAA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}